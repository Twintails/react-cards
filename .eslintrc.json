{
  "extends": "eslint:recommended",
  "plugins": [ "react" ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jasmine":true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": 0,
    "no-prototype-builtins": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-unused-vars": 1,
    "indent": [
      "error", 2,
      { "SwitchCase": 1 ,
      "VariableDeclarator": { "var": 2, "let": 2, "const": 3 } }
    ],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "globals": {
    "console": false,
    "process": false,
    "IS_PRODUCTION": false,
    "IS_DEVELOPMENT": false
  }
}
